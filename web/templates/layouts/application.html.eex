  <link rel="stylesheet" type="text/css" href="//cloud.typography.com/672732/668364/css/fonts.css" />

    <script type="text/javascript" src="/static/assets/bower/jquery/jquery.js"></script>
    <script type="text/javascript" src="/static/assets/bower/angular/angular.js"></script>
    <script type="text/javascript" src="/static/assets/javascripts/app.js"></script>
